{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base", 
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest", 
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": "off",
        "no-console": "off",
        "import/prefer-default-export": "off"
    }
}
